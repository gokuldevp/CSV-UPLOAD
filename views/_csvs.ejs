<link rel="stylesheet" href="/css/_csvs.css">

<table>
    <caption>CSV FILES || Count: <%= count %> || Displayed: <%= displayed %></caption>
    <thead>
        <tr>
            <th>Sl No</th>
            <th>File ID</th>
            <th>File Name</th>
            <th>Open File</th>
        </tr>
    </thead>
    <tbody>
        <% for (i in csvs) { %>
            <% if (parseInt(i) < 100) { %>
            <tr>
                <td><%= parseInt(i) + 1 %></td>
                <td><%= csvs[i].fileName %></td>
                <td><%= csvs[i].originalName %></td>
                <td><a href="/view/<%= csvs[i]._id %>">View</a></td>
            </tr>
            <% } %>
        <% } %>
    </tbody>
</table>
